
"""
Given a valid parentheses string s, return the nesting depth of s. The nesting depth is the maximum number of nested parentheses
"""


def maxDepth(s: str) -> int:
    depth = 0
    max_depth = 0
    
    for char in s:
        if char == '(':
            depth += 1
            max_depth = max(max_depth, depth)
        elif char == ')':
            depth -= 1
            
    return max_depth
